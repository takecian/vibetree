{
  "name": "vibetree",
  "version": "1.3.9",
  "description": "Local task management with Git worktrees and AI integration",
  "main": "bin/vibetree.js",
  "bin": {
    "vibetree": "./bin/vibetree.js"
  },
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev -w server\" \"npm run dev -w client\"",
    "build": "npm run build -w client && npm run build -w server",
    "start": "node bin/vibetree.js",
    "postinstall": "if [ \"$(uname)\" = \"Darwin\" ]; then chmod +x node_modules/node-pty/prebuilds/darwin-arm64/spawn-helper 2>/dev/null || true; chmod +x node_modules/node-pty/prebuilds/darwin-x64/spawn-helper 2>/dev/null || true; fi"
  },
  "keywords": [
    "git",
    "ai",
    "workflow",
    "task-management"
  ],
  "author": "takecian",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "open": "^10.1.0",
    "prompts": "^2.4.2",
    "yargs": "^17.7.2",
    "dotenv": "^16.4.5",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "lowdb": "^7.0.1",
    "node-pty": "1.1.0",
    "socket.io": "^4.8.3",
    "uuid": "^13.0.0",
    "ws": "^8.19.0",
    "typescript": "^5.3.3",
    "@trpc/server": "^11.10.0",
    "better-sqlite3": "^12.6.2",
    "@types/better-sqlite3": "^7.6.13",
    "zod": "^4.3.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^20.11.20",
    "@types/lowdb": "^1.0.15",
    "@types/socket.io": "^3.0.1",
    "@types/uuid": "^9.0.8",
    "@hello-pangea/dnd": "^18.0.1",
    "@tanstack/react-query": "^5.90.21",
    "@trpc/client": "^11.10.0",
    "@trpc/react-query": "^11.10.0",
    "globals": "^15.15.0",
    "glob": "^13.0.3",
    "i18next": "^25.8.7",
    "react-i18next": "^16.5.4",
    "lucide-react": "^0.563.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-diff-view": "^3.3.2",
    "react-router-dom": "^7.13.0",
    "socket.io-client": "^4.8.3",
    "diff": "^8.0.3",
    "@xterm/xterm": "^6.0.0",
    "@xterm/addon-fit": "^0.11.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.20.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "@eslint/js": "^9.20.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "vite": "^5.1.0"
  }
}